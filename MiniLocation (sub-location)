import java.util.*;

public class MiniLocation
{
  private Location location;
  private Item material;

  public MiniLocation(String name, int size)
  {
    Location location = new Location(name, size);
    this.location = location;
  }
  
  public Location getLocation()
  {
    return location;
  }
  
  public void setMaterial(Item material)
  {
    this.material = material;
  }
  
  public void extract(Player player)
  {
    player.addInventory(material);
  }
  
}
